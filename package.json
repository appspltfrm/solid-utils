{
  "name": "@appspltfrm/solid-utils",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "rm -rf dist && npm run build:check && npm run build:js && npm run build:types",
    "build:check": "tsc -p tsconfig.lib.json --noEmit",
    "build:types": "tsc -p tsconfig.lib.json --declaration --emitDeclarationOnly",
    "build:js": "babel src/lib --out-dir dist --extensions \".ts,.tsx\"",
    "install": "node-utils-npm-install-dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-transform-typescript": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@solidjs/router": "^0.7.0",
    "@types/node": "^18.11.18",
    "babel-preset-solid": "^1.6.9",
    "sass": "^1.57.1",
    "ts-patch": "^2.1.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0",
    "hyper-dom-expressions": "^0.35.13"
  },
  "dependencies": {
    "@co.mmons/node-utils": "github:co-mmons/node-utils",
    "solid-element": "^1.6.3",
    "component-register": "^0.8.3",
    "type-fest": "^3.5.3",
    "solid-js": "^1.6.8"
  }
}
